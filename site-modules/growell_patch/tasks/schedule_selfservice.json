{
  "description": "Override configured schedule",
  "input_method": "stdin",
  "files": ["ruby_task_helper/files/task_helper.rb"],
  "parameters": {
    "day": {
      "description": "day of week to schedule override",
      "type": "String[1]"
    },
    "type": {
      "description": "what kind of override",
      "type": "Enum['permanent', 'temporary']",
      "default": "temporary"
    },
    "week": {
      "description": "what occurance",
      "type": "Integer"
    },
    "offset": {
      "description": "the offset",
      "type": "Integer"
    },
    "hours": {
      "description": "when to patch",
      "type": "String[1]"
    },
    "max_runs": {
      "description": "how many time you can patch",
      "type": "Optional[Integer]",
      "default": "1"
    },
    "reboot": {
      "description": "to reboot?",
      "type": "Optional[String[1]]",
      "default": "ifneeded"
    }
  }
}
