<%- | Hash $data | -%>

require 'facter'
require 'json'

vardir = Facter.value(:puppet_vardir)
report = Facter.value(:growell_patch_report)
if report
  _data = { 'growell_patch_report' => report.merge(<%= $data %>) }
else
  _date = { 'growell_patch_report' => <%= $data %> }
end

File.write("#{vardir}/../../facter/facts.d/growell_patch_report.json", JSON.pretty_generate(_data))
